	<h2>Listings</h2>
	<h3>
		<% if @listings.present? %>
		<ul>
			<%= @listings.each do |listing| %>
			<li>
				<p>
					<%= link_to listing.title, listing_path(listing) %> (<%= listing.city %> , <%= listing.state %>)
				</p>
				<p>
					<%= listing.description %>
				</p>
				<p>
					<%= listing.created_at.strftime('%d %B %Y') %>
				</p>
			</li>
			<% end %>
			<% else %>
				<p>No listings found!</p>
			<% end %>
		</ul>
	</h3>