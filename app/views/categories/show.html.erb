<div>
	<%= @category.name %>
	<ul>
		<%= @category.subcategories.each do |subcategory| %>
		<li>
			<%= subcategory.name %>
		</li>
		<% end %>
	</ul>

	<h2>
		<ul>
			<%= @listings.each do |listing| %>
			<li>
				<%= listing.title %>
			</li>
			<li>
				<%= listing.description %>
			</li>
			<li>
				<%= listing.city %>
			</li>
			<li>
				<%= listing.state %>
			</li>
			<% end %>
		</ul>
	</h2>
</div>